{{ docType() | raw }}

<html lang="en">
    <head>

        {% block title %}
            {{ headTitle() | raw }}
        {% endblock %}

        {% block meta %}
            {{
                headMeta().appendName('viewport', 'width=device-width, initial-scale=1.0')
                          .appendName('charset', 'utf-8') | raw
            }}
        {% endblock %}

        {% block stylesheets %}
            {{ headLink()|raw }}
        {% endblock %}

        {% block inlineStyles %}
            {{ headStyle() | raw }}
        {% endblock %}

        {% block scripts %}
            {{ headScript() | raw }}
        {% endblock %}

        {% block inlineScripts %}
            {{ inlineScript() | raw }}
        {% endblock %}

    </head>
<body>

{% if  session() != 'closed' or zfcUserIdentity() is null %}
    {% block disclaimer %}
        {{ disclaimer() }}
    {% endblock disclaimer %}
{% endif %}

    {% block header %}
        <nav class='navbar navbar-default navbar-fixed-top' id='main-nav' role='navigation'>
            <div class="container-fluid'>
                <header class='navbar'>
                    <div class=' navbar-header'>
                        <button type='button' class='navbar-toggle' data-toggle='collapse' data-target='.menu1' style='background:none;'>
                            <span class='icon-bar'></span>
                            <span class='icon-bar'></span>
                            <span class='icon-bar'></span>
                        </button>
                        <button type='button' class='navbar-toggle' data-toggle='collapse' data-target='.navbar-ex2-collapse'>
                            <span class='sr-only'>Toggle navigation</span>
                            <span class='icon-bar'></span>
                            <span class='icon-bar'></span>
                            <span class='icon-bar'></span>
                        </button>
                        <a class='navbar-brand logo' href="/"></a>
                            <img id='logo' src="{{ logo() }}"style="padding-left: 300px">


                        <div class='top-link collapse navbar-collapse navbar-ex2-collapse' id='nav-right' style="margin-top:20px;padding-right: 100px">

                            {% block register %}
                                {{ navigation().menu().renderMenu("register", {"ulClass": "nav top-nav navbar-nav navbar-right login-menu nav-right-login "}) | raw }}
                            {% endblock register %}

                            {% block navigation %}
                                {{ navigation().menu().renderMenu("frontend", {'ulClass': "nav nav navbar-nav"}) | raw }}
                            {% endblock navigation %}

                        </div>
                    </div>
                </header>
            </div>
            <div class='secondary-menu' id='secondary-menu' style="
    height: 38px;
    padding-top: -100px;
    padding-bottom: 0px;
    border-top-width: -50px;
    margin-top: 0;
    border-top-width: 0px;
    padding-top: 0px;

">
                <div class='container' style="height:38px;margin-top: -50;margin-top:20px;">
                    <div class='collapse navbar-collapse menu1 '>
                        {% block subnavigation %}
                            {{ filterNav() }}
                        {% endblock subnavigation %}
                    </div>
                </div>
            </div>
        </nav>
    {% endblock header %}

    {% block headerImage %}{% endblock headerImage %}

    {% block container %}

        <div class='{% block containerClass %}container-fluid{% endblock %}'>
            <div class='row row-no-margin'>
                {% block sidenavigation %}{% endblock sidenavigation %}
                {% block content %}{{ content | raw }}{% endblock content %}
            </div>
        </div>
    {% endblock container %}

    {% block time %}
        <div class="set_time theme-color">
            <div class='container'>
                <div class='pull-left' id='site_clock'></div>
                {% if zfcUserIdentity() %}
                    <div class='pull-right'>
                        <strong>Your Balance is <span id="balance">${{ zfcUserIdentity().getCredit() }}</span></strong>
