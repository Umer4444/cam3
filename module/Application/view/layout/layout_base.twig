    {{ inlineScript() | raw }}

    {% block javascript %}

        {% if zfcUserIdentity() %}
            <script type="text/javascript">

                var user = {
                    id: {{ zfcUserIdentity().getId() }},
                    username: '{{ zfcUserIdentity().getDisplayName() }}'
                };

                {% if inactivity > 0 %}
                    $(document).idleTimeout({
                        inactivity: {{ inactivity }},
                        noconfirm: 10000,
                        sessionAlive: 300000,
                        alive_url: '{{ url('noop') }}',
                        redirect_url: '{{ url('zfcuser/logout') }}'
                    });
                {% endif %}

            </script>

            {% block im %}
                {% include 'chat/chat-sidebar' with {'chat':chat} %}
            {% endblock im %}

        {% endif %}

    {% endblock javascript %}

