                    <div class="row row-leftM-none">
                        <div class="col-sm-5 col-md-5 col-lg-5">
                            <div class="con-profile-pic">
                                <img src="{{ user.getAvatar()}}" alt="pic" class="img-responsive">
                            </div>
                        </div>
                        <div class="col-sm-7 col-md-7 col-lg-7">
                            <small class="txt-muted ">
                                {{ user.getAboutMe() }}
                            </small>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6 col-md-6 col-lg-6">
                    <div class="bg-white border">
                        <table class="table ">
                            <tbody>
                            <tr>
                                <td><strong>Country</strong></td>
                                <td class="text-right">{{ user.getCountry() }}</td>
                            </tr>
                            <tr>
                                <td><strong>Body Type</strong></td>
                                <td class="text-right">{{ user.getBody() }}</td>
