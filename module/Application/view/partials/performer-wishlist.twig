<div class="search_bar visible-xs">
    <form class="form-search form-inline">
        <input type="text" class="form-control input-sm" placeholder="Search">
    </form>
</div>
<div class="content">
    <div class="row">
        <div class="col-lg-12 col-sm-12 col-md-12 col-xs-12">
            <div class="content-block">
                <div class="col-lg-4 col-sm-4 col-xs-12">
                    <div class="">
                        <div class="col-lg-4 col-sm-4 col-xs-12 con-img">
                            <div class="row">
                                <img src="{{ performer.getAvatar() }}" alt="image" class="img-responsive">
                            </div>
                        </div>
                        <div class="col-lg-8 col-sm-8 col-xs-12 padd_large">
                            <div class="row">
                                <h4 class="theme-color"><strong>{{ performer.getUsername() }}</strong></h4>
                                <p class="birthday"><strong>Birthday : {{ performer.getBirthday().format('Y-M-d') }}</strong></p>
                                <p class="wishli">87 items on wishlist</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-8 col-sm-8 col-xs-12">
                    <img src="/assets/images/banner_add_728x90.jpg" alt="banner_add_728x90" width="728" height="90" class="img-responsive">
                </div>

            </div>
            <div class="content-block">
               {% include 'wishlist-list-item' %}
            </div>
        </div>
    </div>
</div>
