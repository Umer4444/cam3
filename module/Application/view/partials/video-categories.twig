<div class="row">
    <div class="club-page form-group">
        <div class="col-lg-12 col-sm-12 col-xs-12 ">
            <h5><strong>POPULAR CATEGORIES</strong></h5>
        </div>
        <div class="row">
            <div class="col-sm-12">
                {% for category in categories %}
                    <div class="col-lg-3 col-sm-6 col-md-3 col-xs-12  block-box ">
                        <div class="">
                            <div class="club-img">
                                <a href="#">
                                    <img src="{{ photos(category.getUser(), 'category_cover')}}" alt="image" class="img-responsive">
                                </a>
                            </div>
                            <div class="club-title">
                                <span class="popcat-title">
                                    <strong>{{ category.name }}</strong>
                                </span> |
                                <span class=""> 1553 videos</span>

                            </div>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
</div>
