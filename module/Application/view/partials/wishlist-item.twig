{% for performer in rows %}
    <div class="col-lg-5ths  col-xs-12  block-box" >
        <div class="block-name">
            <span class="text-left">{{ performer.getScreenName() }}</span>
            <span class="pull-right view_list">
                <a href="/wishlist/item/{{ performer.getId() }}">View Item</a>
            </span>
        </div>
        <div class="con-img">
            <img src="{{ performer.getAvatar() }}" alt="image" class="img-responsive">
        </div>
        <div class="con-detail">
            <div class="detail-left">
                <p>Birthday: {{ performer.getBirthday().format('d-m-my') }}</p>
                <p>Camiaaversary : {{ performer.getBirthday().format('d-m-y') }}</p>
            </div>
            <div class="detail-right">87 items on wishlist</div>
        </div>
    </div>
{% endfor %}