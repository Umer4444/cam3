{% for club in rows %}
    <div class="col-lg-3 col-sm-6 col-md-3 col-xs-12 block-box">
    <div class="">
        <div class="club-title">
            <span class="pull-left">{{ club.getUser().getScreenName() }}</span>
                <span class="pull-right"><img src="images/member.jpg">
                    Members 1234
                </span>
        </div>
        <div class="club-img">
            <img src="{{ club.getUser().getAvatar() }}" alt="image"
                 class="img-responsive">
        </div>
        <div class="club-cont">

            <div class="club-des">
                    <span class="club-des-pa">
                        <a href="#">
                            <i><img src="images/club-post.jpg"></i>
                            post 35548
                        </a>
                    </span>
                    <span class="club-des-pa">
                        <a href="#">
                            <i><img src="images/live-clock.jpg"></i>
                            Live 35548
                        </a>
                    </span>
                    <span class="club-des-pa">
                        <a href="#">
                            <i><img src="images/club-rule.jpg"></i>
                            Club rules
                        </a>
                    </span>
            </div>

            <div class="club-type">
                <strong>Type :</strong> Private
            </div>

            <div class="row">
                <div class="col-sm-6 form-group">
                    <div class="media">
                        <div class="media-left">
                            <a href="#">
                                <img data-holder-rendered="true"
                                     src="images/wall-pic-thumb.jpg"
                                     style="width: 32px; height: 32px;"
                                     class="media-object"
                                     data-src="holder.js/64x64" alt="64x64">
                            </a>
                        </div>
                        <div class="media-body">
                            <h6 class="media-heading">
                                <small class="">Club Founder</small>
                            </h6>
                            <small class="">milliaghi23</small>
                        </div>
                    </div>
                </div>

                <div class="col-sm-6 form-group">
                    <div class="media">
                        <div class="media-left">
                            <a href="# ">
                                <img data-holder-rendered="true"
                                     src="images/wall-pic-thumb.jpg"
                                     style="width: 32px; height: 32px;"
                                     class="media-object"
                                     data-src="holder.js/64x64" alt="64x64">
                            </a>
                        </div>
                        <div class="media-body">
                            <h6 class="media-heading">
                                <small class="">
                                    Club President
                                </small>
                            </h6>
                            <small class="">
                                alexan-dra
                            </small>
                        </div>
                    </div>
                </div>
            </div>
            {#Join Button#}
            <div class="text-center">
                <input type="submit" class="join-club" value="Join Club">
            </div>
        </div>
    </div>
</div>
{% endfor %}
