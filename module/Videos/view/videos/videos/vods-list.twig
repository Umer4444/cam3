{% extends 'layout/frontend' %}


{% block content %}

    {% set useless = paginator(paginator) %}

    <div class="row">
        <div class="club-page form-group">
            <div class="col-lg-12 col-sm-12 col-xs-12">
                <h5><strong>Latest Videos On Demand</strong></h5>
            </div>
            <div class="col-lg-12 col-sm-12 col-xs-12" data-type="infinite">
                {% include 'partial/vod' with {'rows' : paginator.getPaginator().getCurrentItems()} %}
            </div>
        </div>
    </div>

    {{ paginationControl(paginator.getPaginator(), 'Sliding', 'slide-paginator') }}

{% endblock %}